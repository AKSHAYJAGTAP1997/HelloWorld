//Using Recursion
public class Fibonacci{
	public static int fibonacciRecursion(int a,int b, int n){
	if(n == 0){
		return 0;
	}
	int c=a+b;
	System.out.print(c+" ");
	return fibonacciRecursion(b,c,n-1);
	
	}
    public static void main(String args[]) {
	int a=0;
	int b=1;
	int n=10;
	System.out.print(a+" ");
	System.out.print(b+" ");
	fibonacciRecursion(a,b,n-2);
	
}

}

