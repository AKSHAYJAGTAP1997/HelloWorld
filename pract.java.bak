class pract
{
public static void main	(String args[])
{
	int i= 5 ;
	while (i>=1)
	{
		{
			for (int j=i;j>=1;j--)
			{
				System.out.print("*");
			}System.out.println();
	i--;
			
		}
		
	}
}


}













		
			
			